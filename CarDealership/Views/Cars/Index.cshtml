@using CarDealership.Models;

<h1>Car List</h1>

@if (Model.Count == 0)
{
  <p>There are no items in the list.</p>
}
<ul>
  @foreach (Car car in Model)
  {
    <li>Model: @car.MakeModel</li>
    <li>Price: @car.Price</li>
    <li>Miles: @car.Miles</li><br>
  }
</ul>
<a href="/cars/new">Add a new item.</a>